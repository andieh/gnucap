spice
.load c_wave.so

.param p=0
v1 1 0 pwl 0 0 1 2

.store tran v(nodes)
.tran 0 1 1 trace=a > /dev/null

.wstash 'v(1)' w1
.wstash 'v(1)' w2

.wave select stash
.wscale w1 .5
.wwarp w2 2

.echo w1
.wstash w1
.echo w2
.wstash w2

.echo hdd w1 w2 (0)
.wave cmp w1 w2 directed
.echo hdd w1 w2 (1.4)
.wave cmp w2 w1 directed

.end
