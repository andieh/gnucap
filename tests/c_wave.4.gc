spice
.load c_wave.so

.param p=0
v1 1 0 0

.store tran v(nodes)
.tran 0 1 1 trace=a > /dev/null

.wstash 'v(1)' w1
.wstash 'v(1)' w2

.wave select stash
.wwarp w2 2

.echo w1
.wstash w1
.echo w2
.wstash w2

.echo hdd w1 w2 (<1)
.wave cmp w1 w2 directed method=discrete verbose
.echo hdd w1 w2 (1)
.wave cmp w2 w1 directed method=discrete verbose

.echo hd w2 w1
.wave cmp w2 w1 method=discrete verbose
.echo hd w1 w2
.wave cmp w1 w2 method=discrete verbose

.end
