spice
.load c_wave.so

.param p=0
v1 1 0 1
v2 1 2 0

.store dc v(nodes)
.dc v2 0 1 .1 >/dev/null

.wstash 'v(1)' w1
.wstash 'v(2)' w2
.wave select stash
.echo w1
.wstash  w1
.echo w2
.wstash  w2


.echo w1 w2
.wave cmp w1 w2 directed verbose
.echo w2 w1
.wave cmp w2 w1 directed verbose

.dc v2 0 1 1 >/dev/null
.wstash 'v(1)' w3
.wave select stash
.echo w3
.wstash  w3


.echo w3 w2
.wave cmp w3 w2 directed verbose
.echo w2 w3
.wave cmp w2 w3 directed verbose

.end
